<div class="table-wrapper">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Data</th>
        <th>URL</th>
        <th>Durata</th>
        <th>% Successo</th>
        <th><span class="visually-hidden">Azioni</span></th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach(item => { %>
      <tr>
        <td><%- new Intl.DateTimeFormat('it-IT', { dateStyle: "short", timeStyle: "medium" }).format(new Date(item.date)) %></td>
        <td><%- item.url %></td>
        <td><%- `${Math.floor(item.duration / 60)}' ${item.duration % 60 < 10 ? '0' : ''}${item.duration % 60}"` %></td>
        <td><%- item.successRate %>%</td>
        <td>
          <button class="btn btn-primary btn-xs">
            Visualizza
          </button>
          <button class="btn btn-primary btn-xs">
            Scarica
          </button>
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>