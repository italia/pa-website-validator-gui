
<div class="history-title d-flex align-items-start justify-content-between gap-2 mb-3">
  <h1 class="h2 mb-0">Storico scansioni</h1>
  <div class="legend-space d-flex flex-column align-items-end">
    <div class="button-legend d-flex align-items-center justify-content-end gap-3 flex-wrap mb-1">
      <b aria-hidden="true">Legenda:</b>
      <ul class="legend" role="list" title="Legenda">
        <li class="legend-item">
          <svg class="icon flex-shrink-0">
            <use href="#it-note"></use>
          </svg>
          <p>Dettaglio</p>
        </li>

        <li class="legend-item">
          <svg class="icon flex-shrink-0">
            <use href="#it-password-visible"></use>
          </svg>
          <p class="text-nowrap">
            Apri
            <span class="visually-hidden">dettaglio</span>
            in una nuova scheda
          </p>
        </li>

        <li class="legend-item">
          <svg class="icon flex-shrink-0">
            <use href="#it-download"></use>
          </svg>
          <p>Scarica</p>
        </li>

        <li class="legend-item">
          <svg class="icon flex-shrink-0">
            <use href="#it-delete"></use>
          </svg>
          <p>Elimina</p>
        </li>
      </ul>
    </div>
    <%- include('partials/legend', {hideTitle: true}); %>
  </div>
</div>
<%- include('partials/table-history', { items: items });  %>
<%- include('partials/components/pagination', { pagination: pagination }); %>
<%- include('partials/modal-confirm-delete'); %>
<script src="<%- basePathJs %>downloadButtons.js" type="module"></script>
<script src="<%- basePathJs %>setTableHeight.js" type="module"></script>